---
import Layout from "../components/layout.astro"

const posts = await Astro.glob('./blog/*.md')
---

<Layout pageTitle ="Blog" pageDesscriptions="Pagina de lo ultimo en frameworks y noticias">
    <h1>Blog</h1>
    {
        posts.map(post=> {
            return <a href={post.url}>{post.frontmatter.title}</a>
        })
    }

</Layout>